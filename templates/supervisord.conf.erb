[unix_http_server]
file=<%= supervisor_run_dir%>/supervisor.sock   ; (the path to the socket file)

[supervisord]
logfile=<%= supervisor_log%> ; (main log file;default $CWD/supervisord.log)
logfile_maxbytes=500MB       ; (max main logfile bytes b4 rotation;default 50MB)
logfile_backups=10          ; (num of main logfile rotation backups;default 10)
loglevel=info               ; (log level;default info; others: debug,warn,trace)
pidfile=<%= supervisor_run_dir%>/supervisord.pid ; (supervisord pidfile;default supervisord.pid)
nodaemon=false              ; (start in foreground if true;default false)
minfds=1024                 ; (min. avail startup file descriptors;default 1024)
minprocs=200                ; (min. avail process descriptors;default 200)
childlogdir=<%= supervisor_log_dir%> ; ('AUTO' child log dir, default $TEMP)

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix:///<%= supervisor_run_dir%>/supervisor.sock ; use a unix:// URL  for a unix socket

[include]
files = <%= supervisor_log_dir%>/*.ini
